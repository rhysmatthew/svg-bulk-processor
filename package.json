{
  "name": "SVGBulkProcessor",
  "version": "1.0.0",
  "description": "Automatically crops the whitespace from SVGs. Renames to a filename pattern and converts fills to currentColor.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "cropper": "yarn build; yarn server-static; node ./dist/index",
    "server-static": "cp -r ./src/templates ./dist/ && cp -r ./src/static/*.css ./dist/static/"
  },
  "author": "Rhys Matthew",
  "license": "MIT",
  "devDependencies": {
    "@types/argparse": "^2.0.10",
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.6",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "colors-cli": "^1.0.31",
    "express": "^4.18.2",
    "handlebars": "^4.7.7",
    "open": "^8.4.2",
    "prettier": "^2.8.4"
  }
}
